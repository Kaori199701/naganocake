<h1>商品一覧</h1>
<p>商品ID　商品名　税抜価格　ジャンル　ステータス</p>

<% @items.each do |item| %>
<div>
<p>商品ID</p>
<%= item.id %>

<p>商品名</p>
<%= link_to item.item_name, admins_item_path(item.id) %>

<p>税抜価格</p>
<%= item.price %>

<p>ジャンル</p>

<p>ステータス</p>
<% if item.is_active %>
    販売中
<% elsif %>
    販売停止中
<% end %>

</div>
<% end %>



<p>1 2 3 >></p>
